{
  "name": "Block New Tabs",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Block new tabs from opening",
"icons": { "16": "icons/icon-16.png",
           "48": "icons/icon-48.png",
          "128": "icons/icon-128.png" },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon": "icons/icon-128.png",
    "default_title": "Block new tabs"
  }
}
